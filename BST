public class BST{
	protected BSTNode root;
	public void insert(int key){
		if(root==null)
			root= new BSTNode(key);
		else
			root.insert(key);
	}
  public boolean search(int key){
  	if(root==null)
  		return false;
  	else 
  		return root.search(key);
  }
  public void inorder(){
  	if(root==null)
  		return;
  	else
  		root.inorder();
  }
}
