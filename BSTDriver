public class BSTDriver{
	public static void main(String[] args){
		BST b=new BST();
		b.insert(50);
        b.insert(20);
	    b.insert(80);
        b.insert(10);
        b.insert(30);
        b.insert(5);
	    b.insert(15);
	    b.insert(25);
        b.insert(35);
        b.insert(70);
        b.insert(90);
        b.insert(65);
        b.insert(75);
        b.insert(85);
        b.insert(95);
        System.out.println( b.search(43) );
        System.out.println( b.search(70) ); 
	}
}
